@section Scripts{
    <script>
        function ex () {
            var $target = $('.anime'),
                animationClass = 'anime-start',
                offset = $(window).height() * 3 / 4;

            function animeScroll() {
                var documentTop = $(document).scrollTop();

                $target.each(function () {
                    var itemTop = $(this).offset().top;
                    if (documentTop > itemTop - offset) {
                        $(this).addClass(animationClass);
                    }
                    else {
                        $(this).removeClass(animationClass);
                    }
                })
            }
            animeScroll();

            $(document).scroll(function () {
                animeScroll();
            })
        };

        ex();

        $(document).ready(function(){
            $("#detalhestecbtn").on("click", function(){
                $("#detalhestec").html("Utilizo como servidor um PC básico rodando o Windows Server 2019<p>Máquina: Core i5-3470, 4GB de RAM e 500GB de HD</p><p>Todos os sites e este portífolio estão hospedados neste servidor com o runtime do ASP .NET Core e SQL Server, assim como a configuração IIS e do DNS para apontamento dos sites.</p><p>Tudo configurado por mim mesmo :) </p>")
            });
        });
    </script>
